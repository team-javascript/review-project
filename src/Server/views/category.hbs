<main>
    <section class="category-details">
        <h1 class="category__name">{{category.category}}</h1>
        <ul class="category__list">
            {{#each category.reviews}}
            <li class="category__list__item"><a href="/reviews/{{_id}}"> {{title}}</a>
            </li>
            {{/each}}
        </ul>
    </section>
</main>